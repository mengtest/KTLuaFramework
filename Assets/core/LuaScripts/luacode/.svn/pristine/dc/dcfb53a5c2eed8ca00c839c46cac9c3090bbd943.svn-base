Attribute.decl(CharacterInfo, "block_value", 0)
Attribute.decl(CharacterInfo, "block", 0)
-- 精通(潜能)值
Attribute.decl(CharacterInfo, "mastery_value", 0)
Attribute.decl(CharacterInfo, "mastery", 0)
