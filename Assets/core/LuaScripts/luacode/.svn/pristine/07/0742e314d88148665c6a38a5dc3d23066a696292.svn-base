rows[352] = {['tmpl']=10387,['text']='我从小是个孤儿，无名无姓，江湖上叫我阿飞，平时干的就是拿人钱财替人消灾的事情。',['audio']=0,['host_name']=20102014,['anim']='',['another']=10388,['event_id']=0}
