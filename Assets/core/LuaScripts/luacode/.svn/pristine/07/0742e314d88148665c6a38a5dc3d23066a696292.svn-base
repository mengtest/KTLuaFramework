rows[347] = {['tmpl']=10382,['text']='第三个记录完毕，第四个找的是刘若莹',['audio']=0,['host_name']=0,['anim']='',['another']=10000,['event_id']=0}
rows[348] = {['tmpl']=10383,['text']='我乃将军府大小姐，我父亲是朝廷股肱之臣！我们全家都是无辜的！',['audio']=0,['host_name']=20102059,['anim']='',['another']=10384,['event_id']=0}
rows[349] = {['tmpl']=10384,['text']='之前我的夫君居然喜欢那个江宁的妓女，本小姐看不惯想找人杀了她，却在准备下手前父亲被陷害牵连全家，哎，算那个狐狸精走运！',['audio']=0,['host_name']=20102059,['anim']='',['another']=10385,['event_id']=0}
rows[350] = {['tmpl']=10385,['text']='第四个记录完毕，第五个找的是刘飞',['audio']=0,['host_name']=0,['anim']='',['another']=10000,['event_id']=0}
